<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!--圆点属性-->
    <declare-styleable name="XCircleIndicator">
        <!-- 被选中圆点的颜色 -->
        <attr name="fillColor1" format="color" />
        <!-- 未选中圆点的颜色 -->
        <attr name="strokeColor1" format="color" />
        <!-- 圆点的大小 -->
        <attr name="radius1" format="dimension" />
        <!-- 圆点间间距的大小 -->
        <attr name="circleInterval" format="dimension" />

        <!-- 矩形间距的大小 -->
        <attr name="circleWidth" format="dimension" />
    </declare-styleable>
</resources>